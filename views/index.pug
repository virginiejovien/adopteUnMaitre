// PAGE MOT DE PASSE OUBLIE
// ****************************************************************************************
#mot-de-passe-oublie.container.page-content
  .row
    .col-md-6.col-sm-8.col-xs-12.col-md-offset-3
      #locked-screen.panel.panel-primary
        .panel-heading
          h3.panel-title
            | Veuiller entrer votre adresse mail
          button#ferme-mp.close.close-mp(type='button' data-dismiss='modal' aria-hidden='true') X
        .panel-body
          form#form-recup-mp.form-horizontal.needs-validation
            .profile-pic.text-center
              img.img-circle.img-responsive.img-user(src='static/images/default-avatar.png' alt='')
            .form-group
              .col-md-12
                span.input-icon
                  input#mail-recup-mp.form-control.input-recup(type='email' name='mailRecupMp' maxlength='100' size='100' placeholder='Adresse mail' required='' pattern='^([0-9]{9})|([A-Za-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3})$' title='Veuillez fournir une adresse électronique valide: exemple@provider.com')
                  i.fa.fa-envelope-o.darkorange
            .form-group
              .col-md-12
                p#message-recup-mp-mail.lead.mt-5.display-4.text-danger.text-center.alertMsg L&apos;e-mail n&apos;existe pas
                button.btn.btn-info.btn-block(type='submit')  Continuer
// ****************************************************************************************
// PAGE CHANGER DE MOT DE PASSE
// ****************************************************************************************
#change-mp.container.page-content
  .row
    .col-md-6.col-sm-8.col-xs-12.col-md-offset-3
      #locked-screen-m.panel.panel-primary
        .panel-heading
          h3.panel-title
            | Changer votre mot de passe
          button#ferme-mp-change.close.close-mp(type='button' data-dismiss='modal' aria-hidden='true') X
        .panel-body
          form#form-change-mp.needs-validation
            .form-group
              span.input-icon
                input#change-mp-recup.form-control(type='password' name='change-mp-recup' minlength='6' maxlength='12' size='12' placeholder='Mot de passe provisoire' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{5,12}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 6 et 12 caractères')
                i.fa.fa-key.darkorange
            .form-group
              span.input-icon
                input#change-mp1.form-control(type='password' name='change-mp1' minlength='6' maxlength='12' size='12' placeholder='Mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{5,12}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 6 et 12 caractères')
                i.fa.fa-key.darkorange
            .form-group
              span.input-icon
                input#change-mp2.form-control(type='password' name='change-mp2' minlength='6' maxlength='12' size='12' data-match='#change-mp1' data-match-error="Whoops, these don't match" placeholder='Confirmer le mot de passe' required='')
                i.fa.fa-key.darkorange
            .form-group
              .col-md-12
                p#idRecupMpAlertMsg.lead.mt-5.display-4.text-danger.text-center.alertMsg Veuillez saisir le mot de passe provisoire que nous vous avons envoy&eacute; par mail
              button.btn.btn-info.btn-block(type='submit')  Valider
