// ****************************************************************************************
// FORMULAIRE CONNEXION ET INSCRIPTION
// ****************************************************************************************
#formulaire.parallax.filter-black
  .parallax-image
  .small-info
    .col-sm-10.col-sm-push-1.col-md-6.col-md-push-3.col-lg-6.col-lg-push-3
      .card-group.animated.flipInX
        .card
          .card-block
            .center
              h4.m-b-0
                span.icon-text Connexion
              p.text-muted Acc&eacute;der &agrave; votre compte
            // ****************  FORMULAIRE DE CONNEXION **********************
            form#form-connection.needs-validation
              .form-group
                span.input-icon
                  input#pseudo.form-control.is-invalid(type='text' name='LoginPseudo' maxlength='23' size='23' placeholder='Pseudonyme' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{3,23}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 4 et 23 caractères')
                  i.fa.fa-user.darkorange
              .form-group
                span.input-icon
                  input#mp.form-control(type='password' name='LoginPassword' maxlength='12' size='12' placeholder='Mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{5,12}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 6 et 12 caractères')
                  i.fa.fa-key.darkorange
                a#oublie.pull-xs-right(href='#')
                  small Oubli&eacute;?
                p#idLoginAlertMsg.lead.mt-5.display-4.text-danger.text-center.alertMsg Pseudo et/ou mot de passe inconnus
                .clearfix
              .center
                button.btn.btn-azure(type='submit') Se connecter
        .card
          .card-block.center
            h4.m-b-0
              span.icon-text S&apos;inscrire
            p.text-muted Devenir membre
            // ****************  FORMULAIRE D'INSCRIPTION **********************
            form#form-inscription.needs-validation
              .form-group
                span.input-icon
                  input#pseudo-inscription.form-control.control-groupe(type='text' name='pseudoInscrit' maxlength='10' size='10' placeholder='Pseudonyme' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{3,10}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 4 et 23 caractères')
                  i.fa.fa-user.darkorange
              .form-group
                span.input-icon
                  input#mail-inscription.form-control(type='email' name='mailInscrit' maxlength='100' size='100' placeholder='Adresse mail' required='' pattern='^([0-9]{9})|([A-Za-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3})$' title='Veuillez fournir une adresse électronique valide: exemple@provider.com')
                  i.fa.fa-envelope-o.darkorange
              .form-group
                span.input-icon
                  input#mp-inscription.form-control(type='password' name='mpInscrit' minlength='6' maxlength='12' size='12' placeholder='Mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_*\-]{5,12}$' title='Le 1er caractère doit être alphanumérique, les suivants également et la longueur comprise entre 6 et 12 caractères')
                  i.fa.fa-key.darkorange
              .form-group
                span.input-icon
                  input#mp2-inscription.form-control(type='password' name='mp2Inscrit' minlength='6' maxlength='12' size='12' data-match='#mp-inscription' data-match-error="Whoops, these don't match" placeholder='Confirmer le mot de passe' required='')
                  i.fa.fa-key.darkorange
              p#idInscritAlertMsg.lead.mt-5.display-4.text-danger.text-center.alertMsg L&apos;e-mail et/ou le pseudo sont d&eacute;j&agrave; attribu&eacute;s
              button.btn.btn-azure(type='submit') S&apos;inscrire
